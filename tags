!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.9~svn20110310	//
run	slatex.py	/^def run(doc, env, base):$/;"	f
slatex_needed	slatex.py	/^def slatex_needed(target, srcs):$/;"	f
RubberDep	slatex.py	/^class RubberDep (Depend):$/;"	c
__init__	slatex.py	/^	def __init__ (self, mod, target, srcs):$/;"	m	class:RubberDep
run	slatex.py	/^	def run(self):$/;"	m	class:RubberDep
Module	slatex.py	/^class Module (rubber.rules.latex.Module):$/;"	c
__init__	slatex.py	/^	def __init__ (self, doc, dict):$/;"	m	class:Module
add_scheme_file	slatex.py	/^		def add_scheme_file(dict, others=[]):$/;"	f	function:Module.__init__
add_preamble_hook	slatex.py	/^		def add_preamble_hook(name):$/;"	f	function:Module.__init__
